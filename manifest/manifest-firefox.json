{
  "manifest_version": 3,
  "name": "石墨文档导出工具|一键批量导出到本地",
  "version": "1.0.6",
  "description": "从石墨文档全量导出文件到本地，多类型导出格式md、docx、pdf、jpg。文件夹层级保留",
  "permissions": [
    "cookies",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "https://*.shimo.im/*"
  ],
  "background": {
    "scripts": ["browser.js", "background.js"]
  },
  "options_page": "settings.html",
  "action": {
    "default_popup": "popup.html",
    "default_title": "石墨文档导出工具"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}

